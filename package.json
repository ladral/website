{
  "name": "ladral-website",
  "version": "0.1.0",
  "description": "Ladral Website",
  "scripts": {
    "dev": "rimraf dist && parcel src/index.html",
    "build": "rimraf .parcel-cache dist && parcel build src/index.html --no-source-maps",
    "start:ci": "npm run build & npx http-server dist --port 1234",
    "test:e2e": "cypress run",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@parcel/packager-raw-url": "^2.9.1",
    "@parcel/transformer-sass": "^2.9.1",
    "@parcel/transformer-webmanifest": "^2.9.1",
    "@splidejs/splide": "^4.1.4",
    "alpinejs": "^3.12.1",
    "motion": "^10.16.2",
    "parcel": "^2.9.1",
    "rimraf": "^5.0.1"
  },
  "devDependencies": {
    "cypress": "^12.13.0",
    "http-server": "^14.1.1"
  }
}
